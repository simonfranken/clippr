{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CleanUp": {
    "CronExpression": "0 * * * *",
    "Enabled": true,
    "MaxClipAgeHours": 3
  },
  "Hosting": {
    "Url": "http://localhost:8080"
  },
  "ConnectionStrings": {
    "Default": "Server=127.0.0.1;Database=clipprDb;Uid=root;"
  },
  "Authentication": {
    "IdentityService": {
      "IssuerPublicUrl": "http://localhost:8080",
      "IssuerInternalUrl": "http://localhost:8080",
      "Audience": "clippr"
    }
  }
}