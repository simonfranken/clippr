{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "clipprIdentity",
    "Audience": "clippr",
    "ExpiresInMinutes": 60
  },
  "Hosting": {
    "PathBase": "/"
  }
}